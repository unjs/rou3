{
  "name": "@immobiliarelabs/radix3",
  "version": "1.0.0",
  "description": "Lightweight and fast router for JavaScript based on Radix Tree",
  "repository": {
    "type": "git",
    "url": "git@github.com:immobiliare/radix3.git"
  },
  "license": "MIT",
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "scripts": {
    "bench": "node ./benchmark/direct.mjs",
    "bench:http": "node ./benchmark/http.mjs",
    "bench:profile": "0x -o -D benchmark/.profile -- node ./benchmark/direct.mjs",
    "lint": "eslint --fix --ignore-path .gitignore .",
    "style:lint": "eslint src --ext .ts",
    "style:prettier": "prettier \"src/**/*.ts\" --list-different --write",
    "build": "unbuild",
    "dev": "vitest",
    "playground": "jiti ./playground.ts",
    "test": "npm run lint && vitest run",
    "prepare": "node prepare.cjs || echo 'Skipping prepare' && husky install"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.0.0",
    "@commitlint/config-conventional": "^17.0.0",
    "@nuxtjs/eslint-config-typescript": "^11.0.0",
    "@saithodev/semantic-release-backmerge": "^2.0.0",
    "@semantic-release/changelog": "^6.0.1",
    "@semantic-release/commit-analyzer": "^9.0.1",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^8.0.2",
    "@semantic-release/npm": "^9.0.0",
    "@semantic-release/release-notes-generator": "^10.0.2",
    "@typescript-eslint/eslint-plugin": "^5.3.1",
    "@typescript-eslint/parser": "^5.3.1",
    "@vitest/coverage-c8": "^0.24.3",
    "0x": "^5.4.1",
    "autocannon": "^7.10.0",
    "benchmark": "^2.1.4",
    "changelogen": "^0.3.2",
    "eslint": "^8.25.0",
    "eslint-config-prettier": "^8.0.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^8.0.0",
    "is-ci": "^3.0.0",
    "jiti": "^1.16.0",
    "lint-staged": "^13.0.0",
    "listhen": "^0.3.4",
    "markdown-toc": "^1.2.0",
    "ohmyfetch": "^0.4.19",
    "prettier": "^2.0.5",
    "typescript": "^4.8.4",
    "unbuild": "^0.9.2",
    "vitest": "^0.24.3"
  },
  "volta": {
    "node": "16.13.0",
    "npm": "8.1.0"
  }
}
